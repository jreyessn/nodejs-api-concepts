{
  "name": "node-leifer",
  "version": "1.0.0",
  "description": "Curso de Leifer de Nodejs desde 0",
  "main": "src/index.ts",
  "scripts": {
    "serve": "run-p -l start:dev",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc --noEmit",
    "start:dev": "nodemon --exec babel-node --extensions \".ts\" src/index.ts",
    "clean": "rimraf dist",
    "build": "npm run clean && babel src -d dist --ignore=\"./**/*.spec.ts\" --extensions \".ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "mongoose": "^6.4.4",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.6",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
